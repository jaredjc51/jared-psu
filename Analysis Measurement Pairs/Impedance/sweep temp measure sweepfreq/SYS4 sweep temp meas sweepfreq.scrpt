set sweepdevice=HP4980, dmm=HP34401, heater=love_fecube, switch=ni6008
set tempsensor=ktcpl
set temptol=1, temphold=1
set sample=1
set ac=1
sweep temp, start=200, stop=350, step=50
    wait m=10
    measure temp0
    check time0
    measure sweepfreq, start=2e6, stop=20, log=201, rx, med
    measure temp1
    check time1
endsweep
set temp=25